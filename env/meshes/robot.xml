<mujoco model="robot">
  
  <compiler angle="radian"/>

  <option timestep="0.002" gravity="0 0 -9.81"/>
  
  <asset>
    <mesh name="base_link" file="base_link.stl" />
    <mesh name="link1" file="link_1.stl" />
    <mesh name="link2" file="link_2.stl" />
    <mesh name="link3" file="link_3.stl" />
    <mesh name="link4" file="link_4.stl" />
    <mesh name="link5" file="link_5.stl" />
    <mesh name="gripper_base_link" file="gripper_base_link.stl" />
    <mesh name="gripper_left_finger_link" file="gripper_link_left.stl" />
    <mesh name="gripper_right_finger_link" file="gripper_link_right.stl" />
    <mesh name="camera" file="camera.stl" />
  </asset>

  <worldbody>
    <!-- X axis (red) -->
    <geom type="capsule" fromto="0 0 0  0.2 0 0"
          size="0.005" rgba="1 0 0 1"/>

    <!-- Y axis (green) -->
    <geom type="capsule" fromto="0 0 0  0 0.2 0"
          size="0.005" rgba="0 1 0 1"/>

    <!-- Z axis (blue) -->
    <geom type="capsule" fromto="0 0 0  0 0 0.2"
          size="0.005" rgba="0 0 1 1"/>
    <geom name="ground" type="plane" pos="0 0 0" size="5 5 0.1" rgba="0.8 0.8 0.8 1"/>
    <light name="main_light" pos="0 0 5" dir="0 0 -1" diffuse="1 1 1" specular="0.5 0.5 0.5" castshadow="true"/>

    <!-- Base -->
    <body name="base_link" pos="0 0 0.0">
      <inertial pos="0 0.0 0.005" mass="1.7941" diaginertia="0.0048349 0.0083635 0.0048349"/>
      <geom mesh="base_link" type="mesh" rgba="0.8 0.3 0.3 1"/>

      <!-- Link1 -->
      <body name="link1" pos="0 0 0.0592">
        <inertial pos="-0.00038036 0.0 0.03050633" mass="0.23683291" diaginertia="0.00039491 0.00077141 0.00059233"/>
        <geom mesh="link1" type="mesh" rgba="0.1 0.2 0.5 1" />
        <joint name="joint1" type="hinge" axis="0 0 1" range="-2.09 2.09"/>

        <!-- Link2 -->
        <body name="link2" pos="0 0 0.0508">
          <inertial pos="0.00053811 0.0 0.08784697" mass="0.26340123" diaginertia="0.00247044 0.00296718 0.00054702"/>
          <geom mesh="link2" type="mesh" rgba="0.3 0.3 0.3 1" />
          <joint name="joint2" type="hinge" axis="1 0 0" range="-1.57 1.57"/>

          <!-- Link3 -->
          <body name="link3" pos="0 0 0.125">
            <inertial pos="-0.00027016 -0.00023131 0.07622061" mass="0.16450681" diaginertia="0.00129140 0.00138544 0.00015324"/><geom mesh="link3" type="mesh" rgba="0.3 0.3 0.3 1"/>
            <joint name="joint3" type="hinge" axis="1 0 0" range="-1.57 1.57"/>

            <!-- Link4 -->
            <body name="link4" pos="0 0 0.125" >
              <inertial pos="0.0 0.00932713 0.05303399" mass="0.10867011" diaginertia="0.00040570 0.00037552 0.00004410"/>
              <geom mesh="link4" type="mesh" rgba="0.3 0.3 0.3 1"/>
              <joint name="joint4" type="hinge" axis="0 1 0" range="-1.57 1.57"/>

              <!-- Link5 -->
              <body name="link5" pos="0.0 0.0 0.065" >
                <inertial pos="-0.00039819 -0.00306049 0.04570833" mass="0.11311685" diaginertia="0.00034785 0.00035044 0.00003925"/>
                <geom mesh="link5" type="mesh" rgba="0.3 0.3 0.3 1"/>
                <joint name="joint5" type="hinge" axis="0 0 1" range="-1.57 1.57"/>

                <!-- Tool link -->
                <body name="tool_link" pos="0.0 0.0 0.07">
                  <joint name="joint6" type="hinge" axis="1 0 0" range="-1.57 1.57"/>
                  
                  <!-- Gripper -->
                  <body name="gripper_base_link" >
                    <inertial pos="-0.00013639 -0.00931502 0.06177552" mass="0.18022257" diaginertia="0.00087332 0.00090601 0.00018951"/>
                    <geom mesh="gripper_base_link" type="mesh" rgba="0.9 0.9 0.9 1"/>
                    
                    <body name="gripper_left_finger_link" pos="0.0 -0.00758 0.106">
                      <inertial pos="-0.04936121 -0.00165819 0.00790974" mass="0.01666" diaginertia="0.00000859 0.00005127 0.00004602"/>
                      <geom mesh="gripper_left_finger_link" type="mesh" rgba="0.8 0.1 0.1 1" />
                      <!-- <joint name="gripper_left_finger_joint" type="slide" axis="1 0 0" range="-0.019 0"/> -->
                    </body>

                    <body name="gripper_right_finger_link" pos="0.0 -0.00758 0.106">
                      <inertial pos="0.04936121 -0.00165819 0.00790974" mass="0.01666" diaginertia="0.00000859 0.00005127 0.00004602"/>
                      <geom mesh="gripper_right_finger_link" type="mesh" rgba="0.8 0.1 0.1 1" />
                      <!-- <joint name="gripper_right_finger_joint" type="slide" axis="1 0 0"  range="0 0.019"/> -->
                    </body>
                    
                    <body name="camera" pos="0 -0.078 0.07052">
                      <inertial pos="0.0 0.0 -0.0005" mass="0.072" diaginertia="0.00000047 0.00000047 0.00000090"/>
                      <geom mesh="camera" type="mesh" rgba="0.2 0.4 0.7 1"/>
                      <camera name="ee_camera" pos="0 0 0" euler="0 180 0"/>
                    </body>
                    
                    <body name="ee_ref_link" pos="0 0 0.146">
                      <!-- Visual only -->
                      <geom type="sphere" size="0.005" rgba="1 0 0 1" contype="0" conaffinity="0"/>
                    </body>

                  </body>
                </body>
              </body>
            </body>
          </body>
        </body>
      </body>
    </body>
  </worldbody>

  <!-- <tendon>
    <fixed name="gripper_tendon">
      <joint joint="gripper_left_finger_joint" coef="1"/>
      <joint joint="gripper_right_finger_joint" coef="-1"/>
    </fixed>
  </tendon> -->

  <actuator>
    <position name="joint1" joint="joint1" ctrllimited="true" ctrlrange="-2.09 2.09" kp="1" kv="0.1"/>
    <position name="joint2" joint="joint2" ctrllimited="true" ctrlrange="-1.57 1.57" kp="1" kv="0.1"/>
    <position name="joint3" joint="joint3" ctrllimited="true" ctrlrange="-1.57 1.57" kp="1" kv="0.1"/>
    <position name="joint4" joint="joint4" ctrllimited="true" ctrlrange="-1.57 1.57" kp="1" kv="0.1"/>
    <position name="joint5" joint="joint5" ctrllimited="true" ctrlrange="-1.57 1.57" kp="1" kv="0.1"/>
    <position name="joint6" joint="joint6" ctrllimited="true" ctrlrange="-1.57 1.57" kp="1" kv="0.1"/>
    <!-- <position name="gripper" tendon="gripper_tendon" ctrlrange="-0.019 0" kp="1" kv="0.1" /> -->
  </actuator>

  <keyframe>
    <key name="home" qpos="0 0 0 0 0 0"/>
  </keyframe>


</mujoco>